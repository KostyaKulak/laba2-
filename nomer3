#include <iostream>
#include <cstdio>
#include <iostream>
using namespace std;

int how_many_zero(int n){
int zero = 0;

for (int i = 1; i <= n; i++)
{
int i1 = i;
while (i1 % 5 == 0) {
zero = zero + 1;
i1 = i1 / 5;
}
}
return zero;
}
